{
    "compilerOptions": {
        "module": "esnext",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "target": "esnext",
        "allowJs": true,
        "moduleResolution": "node",
        "isolatedModules": true,
        "verbatimModuleSyntax": true,
        "declaration": true,
        "sourceMap": true,
        "declarationMap": true,
        "strict": true,
        "noUncheckedIndexedAccess": true,
        "noImplicitOverride": true,
        "lib": [
            "esnext"
        ]
    }
}
